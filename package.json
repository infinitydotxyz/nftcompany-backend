{
  "name": "nftcompany-backend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "ci-pre-build": "mkdir ./src/creds && mkdir ./creds && cp ./ci/creds-blank/* ./creds && cp ./ci/creds-blank/* ./src/creds",
    "clean": "rimraf dist",
    "build": "npm run clean && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "npm run clean && nest start --watch",
    "dev": "npm run start:dev",
    "start:debug": "npm run clean && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "update:lib": "npm install @infinityxyz/lib@latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mavriklabs/nftcompany-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mavriklabs/nftcompany-backend/issues"
  },
  "homepage": "https://github.com/mavriklabs/nftcompany-backend#readme",
  "dependencies": {
    "@infinityxyz/lib": "^1.49.0",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "axios": "0.26.1",
    "bignumber.js": "9.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "2.8.5",
    "csv-parse": "^5.0.4",
    "date-fns": "^2.28.0",
    "ethers": "5.6.4",
    "express": "4.17.3",
    "express-rate-limit": "6.3.0",
    "firebase-admin": "^10.0.2",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "nodemailer": "6.7.3",
    "pg": "8.7.3",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
