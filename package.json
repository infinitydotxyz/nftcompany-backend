{
  "name": "nftcompany-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fmt": "prettier -c ./src",
    "fmt:fix": "prettier -w ./src",
    "script": "cross-env NODE_PATH=./src ts-node -r dotenv/config ./src/script.ts",
    "start": "cross-env NODE_PATH=./src ts-node -r dotenv/config ./src/main.ts",
    "dev": "npm run update:lib && cross-env NODE_PATH=./src ts-node-dev -r dotenv/config ./src/main.ts",
    "ci-pre-build": "mkdir creds && cp ./ci/creds-blank/* ./creds",
    "compile": "npx tsc --project ./tsconfig.json",
    "build": "npm install && npm run update:lib && rimraf dist && npm run compile",
    "update:lib": "npm update @infinityxyz/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mavriklabs/nftcompany-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mavriklabs/nftcompany-backend/issues"
  },
  "homepage": "https://github.com/mavriklabs/nftcompany-backend#readme",
  "dependencies": {
    "@infinityxyz/lib": "^1.1.2",
    "axios": "0.24.0",
    "bignumber.js": "9.0.2",
    "canvas": "^2.8.0",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.0.3",
    "dotenv": "10.0.0",
    "ethers": "5.5.2",
    "express": "4.17.2",
    "express-fileupload": "^1.2.1",
    "express-jsdoc-swagger": "^1.6.7",
    "express-rate-limit": "5.5.1",
    "firebase-admin": "9.11.1",
    "helmet": "4.6.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mime": "^1.6.0",
    "nodemailer": "6.7.2",
    "pg": "8.7.1",
    "qs": "^6.10.2",
    "reflect-metadata": "^0.1.13",
    "stream-buffers": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/lodash": "^4.14.178",
    "@types/md5": "^2.3.1",
    "@types/node": "17.0.5",
    "@types/nodemailer": "^6.4.4",
    "@types/stream-buffers": "^3.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.5"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true
