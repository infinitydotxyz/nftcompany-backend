{
  "compilerOptions": {
    "rootDir": "src",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "allowJs": true,
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*", ],
      "@utils/*": ["src/utils/*"],
      "@routes/*": ["src/routes/*"],
      "@constants": ["./src/constants.js"],
      "@services/*": ["./src/services/*"]
    }
  },
  "include": ["src/", "src/**/*.json"],
}